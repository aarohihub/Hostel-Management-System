{
  "name": "my-multi-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev-backend": "cd backend && npm run dev",
    "dev-frontend": "cd frontend && npm run dev",
    "dev-dashboard": "cd dashboard && npm run dev",
    "dev-studentdashboard": "cd studentdashboard && npm run dev",
    "dev-staffdash": "cd staffdashboard && npm run dev",
    "dev-all": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\" \"npm run dev-dashboard\" \"npm run dev-studentdashboard\" \"npm run dev-staffdash\""
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
